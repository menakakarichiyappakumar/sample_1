pipeline {
   
    
    stages {
      stage('Initialize'){
        def dockerHome = tool 'myDocker'
        env.PATH = "${dockerHome}/bin:${env.PATH}"
    }
     agent {
        docker { image 'node:mysql' }
    }
        stage('Test') {
            steps {
                sh 'node --version'
            }
        }
    }
}
